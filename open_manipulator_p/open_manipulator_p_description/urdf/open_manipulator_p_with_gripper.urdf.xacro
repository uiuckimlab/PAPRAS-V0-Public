    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.7"/>
  </joint>

  <!-- link 3 -->
  <link name="link3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/link3.stl" scale="0.001 0.001 0.001"/>
