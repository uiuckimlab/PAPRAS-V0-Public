      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.06831 -0.04457 0.00000" rpy="0 0 0"/>
      <!--<origin xyz="-0.05469 0.00043 0.00000" rpy="0 0 0"/>-->
      <mass value="0.43273"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      <!--<inertia ixx="0.00021512" ixy="0.00001009" ixz="0.00000000" iyy="0.00042827" iyz="0.00000000" izz="0.00051436" />-->
    </inertial>
  </link>

  <!-- Joint 6 -->
  <joint name="joint6" type="revolute">
    <parent link="link6"/>
    <child link="end_link"/>
    <origin xyz="0.12300 -0.04500 0.00000" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.7"/>
  </joint>

  <!-- end link -->
  <link name="end_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/end_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/end_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.00337 0.00000 0.00000" rpy="0 0 0"/>
      <mass value="0.01919"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      <!--<inertia ixx="0.00000938" ixy="0.0" ixz="0.0" iyy="0.00000338" iyz="0.0" izz="0.00000635" />-->
    </inertial>
  </link>

  <!-- gripper main fixed joint-->
  <joint name="end_link_fixed" type="fixed">
    <parent link="end_link"/>
    <child link="gripper_main_link"/>
    <origin xyz="0.007 0.0 0.0" rpy="0 0 0" />
  </joint>

  <!-- gripper main link -->
  <link name="gripper_main_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/gripper_main_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/gripper_main_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0.509"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- gripper joint -->
  <joint name="gripper" type="revolute">
    <axis xyz="0 0 1" />		
    <origin xyz="0.0552 -0.0400 0" rpy="0 0 -0.4"/>
    <parent link="gripper_main_link"/>
    <child link="gripper_link"/>
    <limit velocity="6.5" effort="1000" lower="0.0" upper="1.1" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>

  <!-- gripper link -->
  <link name="gripper_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/gripper_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/gripper_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.013"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- gripper joint 1 -->
  <joint name="gripper_sub" type="revolute">
    <axis xyz="0 0 -1" />
    <origin xyz="0.0552 0.0400 0" rpy="0 0 0.4"/>
    <parent link="gripper_main_link"/>
    <child link="gripper_sub_link"/>
    <limit velocity="6.5" effort="1000" lower="0.0" upper="1.1" />
    <dynamics damping="0.1" friction="0.0" />
    <mimic joint="gripper" multiplier="1"/>
  </joint> 

  <!-- gripper 1 link -->
  <link name="gripper_sub_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/gripper_sub_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://open_manipulator_p_description/meshes/gripper_sub_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.013"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- gripper main fixed joint-->
  <joint name="end_effector_joint" type="fixed">
    <parent link="gripper_main_link"/>
    <child link="end_effector_link"/>
    <origin xyz="0.14 0.0 0.0" rpy="0 0 0" />
  </joint>

  <!-- End Effector -->
  <link name="end_effector_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>      
        <box size = "0.01 0.01 0.01"/>
      </geometry>
      <material name="red"/>
    </visual>

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <mass value="0.001" /> 
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Realsense Camera Joint Position -->
  <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0.070 0.032 0.052" rpy="0 0 0"/> 
    <parent link="gripper_main_link"/>
    <child link="camera_link"/>
  </joint>

  <link name="camera_link"/>
    
</robot>
