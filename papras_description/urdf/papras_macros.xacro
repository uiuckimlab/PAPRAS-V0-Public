<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- PAPRAS URDF Macros -->
  <!-- Main URDF File should contain:
        - world link
        - world-env joint
        - env link
        - env-robotn/link1 joint for each mount -->
  <!-- This file contains URDF geometry, transmission data and Gazebo references -->

  <!-- Import Rviz colors -->
  <xacro:include filename="$(find papras_description)/urdf/materials.xacro"/>

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <!-- World -->
  <gazebo reference="world"/>
  <!-- Evn Link -->
  <gazebo reference="env"/>
  <!-- Gazebo Reference macro -->
  <xacro:macro name="RefLink" params="ref">
    <gazebo reference="${ref}">
      <kp>1000000.0</kp>
      <kd>100.0</kd>
      <mu1>30.0</mu1>
      <mu2>30.0</mu2>
      <maxVel>1.0</maxVel>
      <minDepth>0.001</minDepth>
      <material>Gazebo/White</material>
    </gazebo>
  </xacro:macro>
  <!-- ros_control plugin -->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
    </plugin>
  </gazebo>

  <!-- Import Transmission -->
  <!-- Transmission macro -->
  <xacro:macro name="SimpleTransmissionPosition" params="joint n id">
    <transmission name="robot${id}/tran${n}">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="robot${id}/${joint}">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="robot${id}/motor${n}">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
  </xacro:macro>
  <!-- Transmission macro -->
  <xacro:macro name="SimpleTransmissionEffort" params="joint n id">
    <transmission name="robot${id}/tran${n}">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="robot${id}/${joint}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="robot${id}/motor${n}">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
  </xacro:macro>

