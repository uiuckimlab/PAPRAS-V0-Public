      <!-- <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <box size = "0.01 0.01 0.01"/>
        </geometry>
        <material name="red"/>
      </collision> -->

      <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <mass value="0.001" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>
    <gazebo reference="robot${id}/end_effector_link">
      <material>Gazebo/Red</material>
    </gazebo>

  </xacro:macro>

