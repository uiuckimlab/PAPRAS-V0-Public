      </visual>

      <inertial>
        <origin xyz="0.000 0.006 0.011" rpy="0 0 0"/>
        <mass value="0.022"/>
        <!--<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.00001" iyz="0.0" izz="0.0" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>
    <xacro:RefLink ref="robot${id}/gripper_link_r2"/>

    <!-- Left Joint 1 -->
    <joint name="robot${id}/gripper_l1" type="revolute">
      <parent link="robot${id}/gripper_main_link"/>
      <child link="robot${id}/gripper_link_l1"/>
      <origin xyz="0.0 -0.008 0.048" rpy="0 0 0"/>
      <axis xyz="-1 0 0"/>
      <limit velocity="6.5" effort="1000" lower="0.0" upper="1.1" />
      <dynamics damping="0.7"/>
      <mimic joint="robot${id}/gripper" multiplier="${1.0}"/>

    </joint>
    <xacro:SimpleTransmissionEffort n="gripper_l1" joint="gripper_l1" id="${id}"/>

    <!-- rh_p12_rn_l1 -->
    <link name="robot${id}/gripper_link_l1">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find papras_description)/meshes/rh_p12_rn_l1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find papras_description)/meshes/rh_p12_rn_l1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 -0.034 0.004" rpy="0 0 0"/>
        <mass value="0.068"/>
        <!--<inertia ixx="0.00004" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="0.00001" izz="0.00002" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>
    <xacro:RefLink ref="robot${id}/gripper_link_l1"/>

    <!-- Left Joint 2 -->
    <joint name="robot${id}/gripper_l2" type="revolute">
      <parent link="robot${id}/gripper_link_l1"/>
      <child link="robot${id}/gripper_link_l2"/>
      <origin xyz="0.0 -0.0493634 0.0285" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit velocity="6.5" effort="1000" lower="0.0" upper="1.0" />
      <dynamics damping="0.7"/>
      <mimic joint="robot${id}/gripper" multiplier="${1.0/1.1}"/>
    </joint>
    <xacro:SimpleTransmissionEffort n="gripper_l2" joint="gripper_l2" id="${id}"/>


    <link name="robot${id}/gripper_link_l2">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find papras_description)/meshes/fingertip_finray_assy_l.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find papras_description)/meshes/fingertip_finray_assy_l.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 -0.006 0.011" rpy="0 0 0"/>
        <mass value="0.022"/>
        <!--<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.00001" iyz="0.0" izz="0.0" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>


    <!-- gripper main fixed joint-->
    <joint name="robot${id}/end_effector_joint" type="fixed">
      <parent link="robot${id}/gripper_main_link"/>
      <child link="robot${id}/end_effector_link"/>
      <origin xyz="0.0 0.0 0.135" rpy="${pi} ${-pi/2} 0" />
    </joint>

    <!-- End Effector -->
    <link name="robot${id}/end_effector_link">
      <visual>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <box size = "0.01 0.01 0.01"/>
        </geometry>
        <material name="red"/>
      </visual>

      <!-- <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <geometry>
          <box size = "0.01 0.01 0.01"/>
        </geometry>
        <material name="red"/>
      </collision> -->

      <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <mass value="0.001" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>
    <gazebo reference="robot${id}/end_effector_link">
      <material>Gazebo/Red</material>
    </gazebo>

  </xacro:macro>

</robot>
